{
  "name": "@floodteam/components",
  "namespace": "floodteam",
  "version": "1.0.0",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "./dist/esm/index.mjs",
  "es2017": "./dist/esm/index.mjs",
  "types": "./dist/types/components.d.ts",
  "unpkg": "./dist/floodteam/floodteam.esm.js",
  "collection:main": "./dist/collection/index.js",
  "collection": "./dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "deploy:build": "fireenjin env github && npm run docs && npm run presets:build",
    "dev": " stencil build --docs --dev --watch --serve",
    "docs": " stencil build --docs",
    "generate": "fireenjin generate component",
    "start": "npm-run-all --parallel dev presets:watch",
    "test": "stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test:snapshot": "stencil test --e2e --screenshot",
    "test:generate": "stencil test --spec --json --outputFile=src/jest-test-results.json",
    "test:generate:watch": "stencil test --spec --json --outputFile=src/jest-test-results.json --watch",
    "presets:build": "fireenjin presets",
    "presets:watch": "fireenjin presets watch",
    "test:watch": "stencil test --spec --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "dependencies": {
    "@dadesystems/dademobile": "^1.2.1",
    "@ionic-native/core": "^5.32.1",
    "@ionic-native/geolocation": "^5.32.1",
    "@ionic/core": "^5.6.5",
    "@madnesslabs/fireenjin-components": "^1.2.0",
    "@stencil/core": "^2.4.0",
    "algoliasearch": "^4.9.0",
    "chart.js": "^2.9.4",
    "cleave.js": "^1.6.0",
    "clipboard": "^2.0.8",
    "date-fns": "^2.21.1",
    "debounce-decorator": "^1.0.6",
    "json-formatter-js": "^2.3.4",
    "localforage": "^1.9.0",
    "rxjs": "^6.6.3",
    "signature_pad": "^3.0.0-beta.4"
  },
  "devDependencies": {
    "@madnesslabs/fireenjin": "^1.4.14",
    "@madnesslabs/fireenjin-designer": "^1.1.31",
    "@stencil/eslint-plugin": "^0.3.1",
    "@types/chart.js": "^2.9.30",
    "@types/cleave.js": "^1.4.4",
    "@types/googlemaps": "^3.43.2",
    "@types/jest": "^26.0.20",
    "eslint-config-airbnb-typescript": "^10.0.2",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "@stencil/core/testing"
  }
}
